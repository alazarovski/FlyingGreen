setProxyPassword <- function()
{
pwd<-.rs.askForPassword("proxy password?")
proxy = paste0("://",Sys.info()[["user"]],":",pwd,"@proxy.eurocontrol.int:9512")
Sys.setenv(http_proxy=paste0("http",proxy))
Sys.setenv(https_proxy=paste0("http",proxy))
}
setProxyPassword()
